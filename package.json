{
  "name": "trucker",
  "main": "./lib/trucker",
  "version": "1.1.3",
  "description": "IDE refactoring for Javascript: move or rename source files while fixing import statements",
  "keywords": [
    "refactoring",
    "js",
    "coffeescript",
    "imports",
    "require",
    "move",
    "rename",
    "directory"
  ],
  "activationCommands": {
    "atom-workspace": "trucker:move"
  },
  "repository": "https://github.com/ptaylour/atom-trucker",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ramda": "^0.25.0",
    "trucker": "^0.7.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.13.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "prettier": "^1.9.2",
    "temp": "^0.8.3"
  }
}
